{
    "build": {
        "dockerfile": "lieu/Dockerfile"
    },
    "deploy": {
        "startCommand": "./lieu host",
        "replicas": 1,
        "healthcheckPath": "/health",
        "restartPolicy": "On Failure",
        "maxRestartRetries": 10
    },
    "env": {
        "PORT": "10001"
    }
}